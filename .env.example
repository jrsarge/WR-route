# Fast Food Route Optimizer - Environment Configuration
# Copy this file to .env and fill in your values

# ============================================================================
# Google Maps API Configuration (REQUIRED)
# ============================================================================
# Get your API key from: https://console.cloud.google.com/
# Required APIs: Places API, Distance Matrix API, Geocoding API
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# API request rate limiting (requests per second)
# Default: 50 (adjust based on your API quota)
GOOGLE_MAPS_RATE_LIMIT=50

# ============================================================================
# Search Configuration
# ============================================================================
# Default search radius in meters (default: 50km for Salt Lake City area)
DEFAULT_SEARCH_RADIUS=50000

# Maximum number of restaurants to process (default: 1000)
MAX_RESTAURANTS=1000

# ============================================================================
# Optimization Settings
# ============================================================================
# Minimum restaurants required to form a cluster (default: 3)
CLUSTER_MIN_RESTAURANTS=3

# Maximum distance between restaurants in same cluster in km (default: 0.5km)
CLUSTER_EPS_KM=0.5

# Average walking/running speed in km/h (default: 5.0)
WALKING_SPEED_KMH=5.0

# ============================================================================
# Output Configuration
# ============================================================================
# Directory for exported routes and visualizations (default: ./exports)
EXPORT_DIRECTORY=./exports

# Directory for cached data and downloaded information (default: ./data)
DATA_DIRECTORY=./data

# ============================================================================
# Logging Configuration
# ============================================================================
# Log level: DEBUG, INFO, WARNING, ERROR, CRITICAL (default: INFO)
LOG_LEVEL=INFO

# Optional: Path to log file (default: console only)
# LOG_FILE=./logs/fast_food_optimizer.log
